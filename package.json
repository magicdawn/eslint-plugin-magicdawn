{
  "name": "eslint-plugin-magicdawn",
  "version": "0.0.3",
  "description": "eslint plugin for self use",
  "main": "index.js",
  "scripts": {
    "test": "jest",
    "test-cover": "jest --coverage",
    "report-cover": "codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+ssh://git@github.com/magicdawn/eslint-plugin-magicdawn.git"
  },
  "keywords": [
    "eslint",
    "magicdawn",
    "module.export"
  ],
  "author": "magicdawn",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/magicdawn/eslint-plugin-magicdawn/issues"
  },
  "homepage": "https://github.com/magicdawn/eslint-plugin-magicdawn#readme",
  "devDependencies": {
    "codecov": "^1.0.1",
    "eslint": "^3.10.0",
    "jest": "^24.1.0",
    "husky": "latest",
    "lint-staged": "latest",
    "prettier": "latest"
  },
  "peerDependency": {
    "eslint": ">3"
  },
  "files": [
    "package.json",
    "README.md",
    "CHANGELOG.md",
    "LICENSE",
    "index.js",
    "lib"
  ],
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,less,vue}": [
      "prettier --write",
      "git add"
    ]
  }
}
